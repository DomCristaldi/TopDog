  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/target/debug/top_dog",
      "args": ["--features 'vulkan'"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
    },
    {
      "name": "(gdb) Release",
      //"type": "cppvsdbg",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/target/release/top_dog",
      "args": ["--features 'vulkan'"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
    },
  ]
}